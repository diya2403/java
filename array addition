public class MatrixAdditionExample{  
public static void main(String args[]){  

//TWO ARRAYS ARE CREATED 
int a[][]={{1,2,3},{4,3,2},{3,4,5}};    
int b[][]={{1,7,4},{2,0,3},{1,1,4}};  

//AARAY IS CREATED TO STORE RESULT  
int c[][]=new int[3][3]; 

//ADDITION OF ARRAYS
for(int i=0;i<3;i++){    
for(int j=0;j<3;j++){    
c[i][j]=a[i][j]+b[i][j];  
System.out.print(c[i][j]+" ");    
}    
System.out.println();
}}}   
